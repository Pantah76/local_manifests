<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!--Remove Project-->
    <remove-project name="pixys-gitlab/vendor_gms" />
    <remove-project name="PixysOS/vendor_pixys" />
    
    <!--Remotes-->
    <remote name="source"        fetch="https://github.com/Pantah76"/>
    <remote name="gitlab"        fetch="https://gitlab.com/" />

    <!--Devices Trees-->
    <project name="android_device_google_pantah"                        path="device/google/pantah"             remote="source"      revision="pixys" />
    <project name="android_device_google_gs201"                         path="device/google/gs201"              remote="source"      revision="pixys" />

    <!--Vendors Trees-->
    <project name="proprietary_vendor_google_panther"                   path="vendor/google/panther"            remote="source"      revision="lineage-21" />
    <project name="proprietary_vendor_samsung_cheetah"                  path="vendor/samsung/cheetah"           remote="source"      revision="lineage-21" />

    <!--Kernel Tree-->
    <project name="android_kernel_google_gs201"                         path="kernel/google/gs201"              remote="source"      revision="lineage-21" />

    <!-- Dependencies -->
    <project name="packages_apps_PixelParts"                            path="packages/apps/PixelParts"         remote="source"      revision="udc" />

    <!-- Vendor GMS -->
    <project path="vendor/gms" name="DerpFest/vendor_gms" remote="gitlab" revision="14" clone-depth="1" />
</manifest>
